<div class="row outlay">
  <div class="col-md-10">
    <h1><%= @application.name %></h1>
    <h3><%= @application.url %></h3>
    <% @events.each do |event| %>
      <p><%= event[0] %></p>
    <% end %>
  </div>
  <div class="col-md-2 little">

  <%= link_to "Delete Application", [current_user, @application], method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this application?'} %>
  <%= link_to "Back to List", user_applications_path(current_user), class: 'btn btn-info' %>
  <p>Total Number Of Events: <%= @events.length %> </p>

  </div>
</div>
